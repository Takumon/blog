  },
  "scripts": {
    "preoptimizeImage": "run-s preoptimizeImage:**",
    "preoptimizeImage:thumbnails": "node ./scripts/optimize-thumbnaisl.js",
    "preoptimizeImage:markdownImages": "node ./scripts/optimize-markdown-images.js",
    "compressImage": "node ./scripts/compress-image.js",
    "dev": "run-s develop",
    "develop": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .ts,.tsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "install-local-gatsby-plugins": "run-p install-local-gatsby-plugins:**",
    "install-local-gatsby-plugins:gatsby-remark-and-qiita": "cd ./plugins/gatsby-remark-and-qiita && npm i",
    "install-local-gatsby-plugins:gatsby-remark-headings-detail": "cd ./plugins/gatsby-remark-headings-detail && npm i",
    "build": "run-s install-local-gatsby-plugins build:gatsby",
    "build:gatsby": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby-parallel-runner build --log-pages",
    "local-build": "run-s install-local-gatsby-plugins local-build:gatsby",
    "local-build:gatsby": "gatsby build --log-pages",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "rm": "gatsby clean && rd /s ./public",
    "lhci": "lhci autorun"
  }
}
